<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeewan">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:hardwareAccelerated="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".menuIeams.FeedbackSubmitted"></activity>
        <activity android:name=".menuIeams.AboutUs" />
        <activity android:name=".menuIeams.Send_Feedback" />
        <activity android:name=".onboarding.screens.OnBoarding" />
        <activity android:name=".profile.ProfileForm" />
        <activity android:name=".MainScreenActivity" />
        <activity android:name=".authentication.loginginscreen" />
        <activity android:name=".authentication.signup" />
        <activity android:name=".authentication.ManageOtp" />
        <activity
            android:name=".authentication.Mainlogin"
            android:label="home" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>